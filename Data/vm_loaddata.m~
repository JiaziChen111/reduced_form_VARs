%Data set:
%Output, Inflation, Interest Rates, InvVelocity  

load datampshock.txt
YY=datampshock;
ti=linspace(1964,0.2, size(YY,1))';
nobs=size(YY,1);

%Convert velocity into real money balances

YY(:,4)=YY(:,4)+YY(:,1);


% Plot the data

% Attributes 
linesty={'-','-','-','-'};
color={'b','g','k','r'};
marker={'none','none','none','none'};
legend_list = {'Output', 'Inflation', 'Interest rate', 'Real money'};

% xaxis
time = 1959:0.2:

figure(1);
p = plot(YY, 'linewidth', 1);
size_p=size(p);
for i=1:size_p(1);
    set(p(i),'LineStyle',linesty{i},'Color',color{i},'Marker',marker{i});
    xlabel("Time")
    legend(legend_list,'orientation','horizontal');
end